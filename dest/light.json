{
  "name": "Themelier Light",
  "include": "./common.json",
  "colors": {
    "editor.foreground": "#ABB2BF",
    "editor.background": "#292C33",
    "tab.activeBackground": "#292C33",
    "editorWidget.background": "#292C33",
    "editorSuggestWidget.background": "#292C33",
    "editorHoverWidget.background": "#262626",
    "editorHoverWidget.border": "#141414",
    "editorSuggestWidget.border": "#141414",
    "editorSuggestWidget.selectedBackground": "#212121",
    "peekViewEditor.background": "#22252A",
    "input.background": "#22252A",
    "tab.inactiveBackground": "#22252A",
    "editorGroup.background": "#22252A",
    "editorGroupHeader.tabsBackground": "#22252A",
    "editor.selectionBackground": "#3F4450",
    "editor.lineHighlightBackground": "#3b3b3b",
    "editor.findMatchHighlightBackground": "#343434",
    "editor.findMatchBackground": "#282828",
    "editorWhitespace.foreground": "#4C5262",
    "editorIndentGuide.background": "#4C5262",
    "editorLineNumber.foreground": "#4C5262",
    "activityBarBadge.background": "#577AC7",
    "activityBarBadge.foreground": "#dbdbdb",
    "sideBar.background": "#22252A",
    "sideBarSectionHeader.background": "#191919",
    "list.inactiveSelectionBackground": "#060606",
    "list.activeSelectionBackground": "#060606",
    "list.activeSelectionForeground": "#707070",
    "list.focusBackground": "#060606",
    "list.hoverBackground": "#131313",
    "statusBar.background": "#292C33",
    "statusBar.foreground": "#757575",
    "statusBar.noFolderBackground": "#292C33",
    "statusBarItem.hoverBackground": "#212121",
    "titleBar.activeBackground": "#292C33",
    "activityBar.background": "#22252A",
    "activityBar.foreground": "#707070",
    "notification.background": "#22252A",
    "button.background": "#577AC7"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#ABB2BF",
        "background": "#292C33",
        "lineHighlight": "#3b3b3b",
        "selection": "#3F4450",
        "findHighlight": "#343434",
        "guide": "#4C5262"
      }
    },
    {
      "name": "variable, definition, punctuation",
      "scope": [
        "variable",
        "punctuation.definition.variable",
        "meta.definition.variable variable",
        "variable.parameter",
        "meta.brace",
        "punctuation.definition.array",
        "punctuation.definition.binding-pattern",
        "punctuation.definition.block",
        "punctuation.definition.dictionary",
        "punctuation.definition.tag",
        "punctuation.section",
        "punctuation.separator",
        "punctuation.support",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "string",
      "scope": [
        "string",
        "markup.inline.raw",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#5C6370"
      }
    },
    {
      "name": "property, htmlTag, cssProperty, mkHeading",
      "scope": [
        "support.type.property-name",
        "support.variable.property",
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key",
        "entity.name.tag",
        "support.type.property-name.css",
        "support.type.property-name.sass",
        "support.type.property-name.scss",
        "markup.heading",
        "markup beginning.punctuation.definition"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "function, htmlId, mkLinkText",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.other.attribute-name.id",
        "string.other.link.title"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "keyword, mkItalic",
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "storage",
      "scope": [
        "storage",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "operator, mkLinkUrl",
      "scope": [
        "keyword.operator",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "support, class, reserved",
      "scope": [
        "support",
        "storage.type.core",
        "entity.name.class",
        "entity.other.inherited-class",
        "entity.name.type",
        "storage.class",
        "variable.language"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "constant, htmlAttr, mkBold",
      "scope": [
        "constant",
        "keyword.other.unit",
        "support.constant",
        "variable.other.constant",
        "entity.other.attribute-name",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    }
  ]
}