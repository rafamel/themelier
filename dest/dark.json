{
  "name": "Themelier Dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "sideBar.background": "#1E1E1E",
    "titleBar.activeBackground": "#1E1E1E",
    "statusBar.background": "#222222",
    "peekViewEditor.background": "#222222",
    "activityBar.background": "#222222",
    "tab.inactiveBackground": "#282828"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "variable, definition, punctuation",
      "scope": [
        "support.variable",
        "variable",
        "punctuation.definition.variable",
        "meta.definition.variable variable",
        "variable.parameter",
        "meta.brace",
        "punctuation.definition.array",
        "punctuation.definition.binding-pattern",
        "punctuation.definition.block",
        "punctuation.definition.dictionary",
        "punctuation.definition.string",
        "punctuation.definition.tag",
        "punctuation.section",
        "punctuation.separator",
        "punctuation.support",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "property, htmlTag, muHeading",
      "scope": [
        "support.type.property-name",
        "support.variable.property",
        "variable.other.property",
        "variable.other.object.property",
        "entity.name.tag",
        "markup.heading",
        "markup beginning.punctuation.definition"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "function, htmlId, muLinkText",
      "scope": [
        "entity.name.function",
        "entity.other.inherited-class",
        "support.function",
        "entity.other.attribute-name.id",
        "string.other.link.title"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "keyword, storage, muItalic",
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "storage",
        "storage.type",
        "keyword.operator.new",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "operator, muLinkUrl",
      "scope": [
        "keyword.operator",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "support, class, reserved",
      "scope": [
        "support",
        "entity.name.class",
        "entity.name.type",
        "variable.language"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "constant, htmlAttr, muBold",
      "scope": [
        "constant",
        "keyword.other.unit",
        "support.constant",
        "entity.other.attribute-name",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#5C6370"
      }
    },
    {
      "name": "string",
      "scope": [
        "string",
        "markup.inline.raw",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "Bolding",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italizing",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}