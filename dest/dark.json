{
  "name": "Themelier Dark",
  "include": "./common.json",
  "colors": {
    "editor.background": "#1E1E1E",
    "tab.activeBackground": "#1E1E1E",
    "editorWidget.background": "#1E1E1E",
    "editorSuggestWidget.background": "#1E1E1E",
    "editorHoverWidget.background": "#262626",
    "editorHoverWidget.border": "#373737",
    "editorSuggestWidget.border": "#373737",
    "editorSuggestWidget.selectedBackground": "#2b2b2b",
    "peekViewEditor.background": "#222222",
    "input.background": "#222222",
    "tab.inactiveBackground": "#222222",
    "editorGroup.background": "#222222",
    "editorGroupHeader.tabsBackground": "#222222",
    "sideBar.background": "#1E1E1E",
    "sideBarSectionHeader.background": "#2b2b2b",
    "list.inactiveSelectionBackground": "#3e3e3e",
    "list.activeSelectionBackground": "#3e3e3e",
    "list.activeSelectionForeground": "#f7f7f7",
    "list.focusBackground": "#3e3e3e",
    "list.hoverBackground": "#313131",
    "statusBar.background": "#1E1E1E",
    "statusBar.foreground": "#f7f7f7",
    "statusBar.noFolderBackground": "#1E1E1E",
    "statusBarItem.hoverBackground": "#2b2b2b",
    "titleBar.activeBackground": "#1E1E1E",
    "activityBar.background": "#222222",
    "activityBar.foreground": "#fbfbfb",
    "notification.background": "#222222"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#ABB2BF",
        "background": "#1E1E1E"
      }
    },
    {
      "name": "variable, definition, punctuation",
      "scope": [
        "variable",
        "punctuation.definition.variable",
        "meta.definition.variable variable",
        "variable.parameter",
        "meta.brace",
        "punctuation.definition.array",
        "punctuation.definition.binding-pattern",
        "punctuation.definition.block",
        "punctuation.definition.dictionary",
        "punctuation.definition.tag",
        "punctuation.section",
        "punctuation.separator",
        "punctuation.support",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "string",
      "scope": [
        "string",
        "markup.inline.raw",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#5C6370"
      }
    },
    {
      "name": "property, htmlTag, cssProperty, mkHeading",
      "scope": [
        "support.type.property-name",
        "support.variable.property",
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key",
        "entity.name.tag",
        "support.type.property-name.css",
        "support.type.property-name.sass",
        "support.type.property-name.scss",
        "markup.heading",
        "markup beginning.punctuation.definition"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "function, htmlId, mkLinkText",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.other.attribute-name.id",
        "string.other.link.title"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "keyword, mkItalic",
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "storage",
      "scope": [
        "storage",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "operator, mkLinkUrl",
      "scope": [
        "keyword.operator",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "support, class, reserved",
      "scope": [
        "support",
        "storage.type.core",
        "entity.name.class",
        "entity.other.inherited-class",
        "entity.name.type",
        "storage.class",
        "variable.language"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "constant, htmlAttr, mkBold",
      "scope": [
        "constant",
        "keyword.other.unit",
        "support.constant",
        "variable.other.constant",
        "entity.other.attribute-name",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    }
  ]
}