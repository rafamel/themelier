{
  "name": "themelier",
  "displayName": "Themelier",
  "description": "Easy to personalize syntax and UI themes with awesome out-of-the-box presets for VSCode",
  "version": "0.0.1",
  "publisher": "rafamel",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.reload",
    "onCommand:extension.choose"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.reload",
        "title": "Reload Themelier"
      },
      {
        "command": "extension.choose",
        "title": "Choose Themelier Theme"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
